{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  },
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "member-access": false,
    "object-literal-sort-keys": false
  }
}
